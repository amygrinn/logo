{
  "name": "@taylorgrinn/logo",
  "version": "4.0.2",
  "description": "Logo for Taylor",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "sass"
  ],
  "browserslist": [
    "defaults"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "alloy",
      "alloy/react",
      "alloy/typescript"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "eol-last": [
        "error",
        "always"
      ]
    },
    "ignorePatterns": [
      "lib/*",
      "dist/*",
      "webpack.config.js",
      "html-webpack-exclude-tags-plugin.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "ts": "tsc",
    "ts:w": "npm run ts -- -w",
    "sass": "sass -I node_modules --style=compressed --no-source-map sass/index.scss lib/tay-logo.min.css",
    "sass:w": "npm run sass -- --watch",
    "webpack": "webpack",
    "webpack:w": "npm run webpack -- -w",
    "plugin": "run-p ts sass webpack",
    "plugin:w": "run-p ts:w sass:w webpack:w",
    "demo": "webpack --config demo/webpack.config",
    "demo:w": "npm run demo -- -w",
    "build": "run-s clean plugin demo",
    "serve": "http-server dist",
    "reload": "reload -d dist -b",
    "start": "npm-run-all build -p plugin:w demo:w reload",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run lint:fix",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "prettier": "npm run prettier:fix",
    "quality:check": "run-s --print-label --silent lint:check prettier:check",
    "quality:fix": "run-p --print-label --silent lint:fix prettier:fix",
    "quality": "npm run quality:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorgrinn/logo.git"
  },
  "keywords": [
    "react",
    "login",
    "webpack"
  ],
  "author": "Taylor Grinn",
  "license": "ISC",
  "peerDependencies": {
    "react": "18"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-config-alloy": "^4.7.0",
    "eslint-plugin-react": "^7.31.8",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^20.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reload": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.1.4"
  }
}
